      "source": [
        "import numpy as np\n",
        "P0 = 1000\n",
        "r = 0.03\n",
        "t = np.linspace(0, 20, 201)\n",
        "P = P0 * np.exp(r * t)\n",
        "dP_dt = r * P\n",
        "\n",
        "P_20 = P0 * np.exp(r * 20)\n",
        "dP_dt_20 = r * P_20\n",
        "\n",
        "print(\"Population at t=20:\", P_20)\n",
        "print(\"Growth rate at t=20:\", dP_dt_20)\n",
        "\n"
      ]
